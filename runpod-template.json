{
  "name": "ComfyUI elfatherbrown template (Community)",
  "imageName": "ghcr.io/elfatherbrown/aidock-comfyui:master",
  "containerDiskInGb": 12,
  "ports": ["8188/http,8888/http,22/tcp"],
  "dockerArgs": "--env=COMMANDLINE_ARGS=--lowvram --attention-slicing --sequential-offload",
  "env": {
    "WORKSPACE": "/workspace",
    "WEB_ENABLE_AUTH": "false",
    "COMFYUI_PORT_HOST": "8188",
    "JUPYTER_PORT_HOST": "8888",
    "SSH_PORT_HOST": "22",
    "HF_TOKEN": "${{ secrets.HF_TOKEN }}",
    "CIVITAI_TOKEN": "${{ secrets.CIVITAI_API_KEY }}",
    "AUTO_UPDATE": "true"
  },
  "volumeInGb": 0,
  "volumeMountPath": "/workspace",
  "description": "ComfyUI Stable Diffusion backend and GUI (Community Cloud)"
}
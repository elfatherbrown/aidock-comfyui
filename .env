# Python and PyTorch versions
PYTHON_VERSION=3.10
PYTORCH_VERSION=2.4.1

# Workspace configuration
WORKSPACE=/workspace
WORKSPACE_SYNC=false

# Port configurations
SSH_PORT_HOST=2222
SERVICEPORTAL_PORT_HOST=1111
COMFYUI_PORT_HOST=8188
JUPYTER_PORT_HOST=8888
SYNCTHING_UI_PORT_HOST=8384
SYNCTHING_TRANSPORT_PORT_HOST=22999

# Web interface settings
WEB_ENABLE_AUTH=true
WEB_ENABLE_HTTPS=false
WEB_USER=user
WEB_PASSWORD=password

# Auto-update settings
AUTO_UPDATE=true

# Optional tokens (uncomment and set if needed)
#CIVITAI_TOKEN=your_token_here
#HF_TOKEN=your_token_here 